<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>FeeManage</display-name>
  <welcome-file-list>
    <welcome-file>welcome.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  <servlet-name>reg</servlet-name>
  <servlet-class>com.sam.BERI.fee.controller.RegController</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>reg</servlet-name>
	<url-pattern>/reg.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>log</servlet-name>
  	<servlet-class>com.sam.BERI.fee.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>log</servlet-name>
  	<url-pattern>/login.do</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>vb</servlet-name>
  	<servlet-class>com.sam.BERI.fee.controller.ViewController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>vb</servlet-name>
  	<url-pattern>/view.do</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>tf</servlet-name>
  	<servlet-class>com.sam.BERI.fee.controller.TransferController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>tf</servlet-name>
    <url-pattern>/trans.do</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>ms</servlet-name>
  	<servlet-class>com.sam.BERI.fee.controller.StatementController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ms</servlet-name>
  	<url-pattern>/mini.do</url-pattern>
  </servlet-mapping>
</web-app>